{
    "name": "sbsedv/input-converter-bundle",
    "description": "A Symfony ^6.1 bundle that adds the raw request body input to the http-foundation InputBag.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Burger",
            "homepage": "https://github.com/danielburger1337"
        }
    ],
    "autoload": {
        "psr-4": {
            "SBSEDV\\Bundle\\InputConverterBundle\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1",
        "sbsedv/input-converter": "^3.0",
        "symfony/framework-bundle": "^6.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.13",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.4",
        "phpstan/phpstan-symfony": "^1.1"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
